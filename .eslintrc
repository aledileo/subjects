{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-indent": [1, "tab"],
    "react/jsx-indent-props": [1, "tab"],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "react/require-default-props": [0]
  }
}
